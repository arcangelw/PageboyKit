# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'PageboyKit' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PageboyKit
  pod 'Pageboy', :git => 'https://github.com/arcangelw/Pageboy.git', :branch => 'master'
  pod 'SwiftLint'
  target 'PageboyKitTests' do
    # Pods for testing
  end

end

